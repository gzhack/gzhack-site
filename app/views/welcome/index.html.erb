    </ul>
  </nav>
</header>

<section class="introduction-banner">
  <div id="js_slider" class="slider">
    <!-- Slides Container -->
    <div u="slides" class="slider-inner" style="">
      <div>
        <%= image_tag "homepage-slider-1.jpg", u: "image" %>
      </div>
      <div>
        <%= image_tag "homepage-slider-2.jpg", u: "image" %>
        <a class="signup_button" href="http://omg.com">
          <div>报名参赛</div>
        </a>
      </div>
      <!--<div u="image" class="homepage-slider-1"></div>
      <div u="image" class="homepage-slider-2"></div>-->
