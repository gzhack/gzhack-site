<script>
  var headerElem = document.getElementsByTagName('header')[0];
  var bannerElem = document.querySelector('.introduction-banner');
  var windowHeight = window.innerHeight;
  var headerHeight = headerElem.offsetHeight;

  bannerElem.style.height = (windowHeight - headerHeight) + 'px';

  var options = {
    //$AutoPlay: true,
    $PauseOnHover: 0,
    $Duration: 1000,
    $FillMode: 2,
    $BulletNavigatorOptions: {
      $Class: $JssorBulletNavigator$,
      $ChanceToShow: 2
    }
  };
  var jssor_slider = new $JssorSlider$('js_slider', options);
</script>

